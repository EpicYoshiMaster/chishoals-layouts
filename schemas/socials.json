{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "youtube": {
            "type": "string",
            "description": "Youtube Channel",
            "default": "@SquidWestLANs"
		},
		"twitter": {
			"type": "string",
			"description": "Twitter Username",
			"default": "@SquidWest"
		},
		"bluesky": {
			"type": "string",
			"description": "Bluesky Username",
			"default": "@squidwest.bsky.social"
		},
		"discord": {
			"type": "string",
			"description": "Discord Link",
			"default": "discord.gg/trdHY59F2u"
		}
    },
    "required": ["youtube", "twitter", "bluesky", "discord"]
}