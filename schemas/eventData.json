{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "eventName": {
            "type": "string",
            "description": "The current event name.",
            "default": "My Event Name"
        },
        "eventLocation": {
            "type": "string",
            "description": "The Location the event is taking place.",
            "default": "My Event Location"
        },
        "eventNumber": {
            "type": "number",
            "description": "The current event number.",
            "default": 1
        },
        "nextEventName": {
            "type": "string",
            "description": "The next event name.",
            "default": "My Next Event Name"
        },
        "nextEventLocation": {
            "type": "string",
            "description": "The Location the next event is taking place.",
            "default": "My Next Event Location"
        },
        "nextEventNumber": {
            "type": "number",
            "description": "The next event number.",
            "default": 2
        },
        "nextEventDate": {
            "type": "string",
            "description": "The next event date.",
            "default": "January 1, 2024"
        },
        "setupTeam": {
            "type": "array",
            "items": { "type": "string" },
            "description": "The list of Setup and Teardown volunteers helping with the event.",
            "default": ["Volunteer #1", "Volunteer #2", "Volunteer #3"]
        },
        "commentaryTeam": {
            "type": "array",
            "items": { "type": "string" },
            "description": "The list of Commentators helping with the event.",
            "default": ["Commentator #1", "Commentator #2", "Commentator #3"]
        },
        "staffTeam": {
            "type": "array",
            "items": { "type": "string" },
            "description": "The list of SquidWest TOs and Staff.",
            "default": ["Staff #1", "Staff #2", "Staff #3"]
        },
        "eventTeam": {
            "type": "array",
            "items": { "type": "string" },
            "description": "The list of Event TOs for this event.",
            "default": ["Event TO Name"]
        }
    },
    "required": ["eventName", "eventLocation", "eventNumber", "nextEventName", "nextEventLocation", "nextEventNumber", "nextEventDate", "setupTeam", "commentaryTeam", "staffTeam", "eventTeam"]
}